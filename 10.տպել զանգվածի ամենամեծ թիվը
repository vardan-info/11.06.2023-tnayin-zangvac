public class Main {
    public static void main(String[] args) {
int[]ints= {10,20,30,40,50};
        int max = Integer.MIN_VALUE;
        for(int currentValue:ints){
            if(currentValue >max){
                max=currentValue;//<--ընթացիկ արժեքը
            }
        }
        System.out.println("max=" + max);
 }
    }
////max=50
