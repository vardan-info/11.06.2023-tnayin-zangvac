public class Main {
    public static void main(String[] args) {
        int[] array = new int[]{5, 10, 15, 20, 21,22};
        int sum = 0;
        for (int i = 0; i < array.length; i++) {
       if(array[i] %5==0){
           sum+=array[i];
       }


        }

        System.out.println(sum);
    }
}
