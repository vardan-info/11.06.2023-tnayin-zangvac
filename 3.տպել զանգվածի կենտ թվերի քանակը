public class Main {
    public static void main(String[] args) {
            int[] array = {1, 2, 5, 6, 7, 8, 9};
            int count = 0;
            for (int i = 0; i < array.length; i++) {
                if (array[i] % 2 != 0) {
                    count++;



                }
            }
        System.out.println(count);
        }
    }

