public class Main {
    public static void main(String[] args) {
           int []array = new int[5];
           array[1]= 0;
           array [2] = 1;
           array [3]= 2;
           array [4]= 3;
           array[0]=4;
           int sum =0;
           for(int i = 0; i< array.length;i++){
               sum+=array[i];
           }
        System.out.println(sum);
        }

    }
