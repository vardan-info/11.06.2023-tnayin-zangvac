public class Main
 {
	public static void main(String[] args) {
 int  []array = {1,2,5,44,4,8,24,99};
        int sum = 0;
        for(int i = 0; i < array.length;i++){
            if(array[i] % 2==0){
                sum*=i;

            }

        }
         System.out.println(sum);

        }
        }
