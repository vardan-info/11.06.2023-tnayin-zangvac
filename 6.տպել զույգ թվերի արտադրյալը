public class Main {
    public static void main(String[] args) {
int array[] = {1,2,6,4,8,24,22,3,5,7};
  int sum =0;

        for(int i = 0; i<array.length; i++){
            if(array[i]  %2==0){
            sum += i;
               
            }
             
        }
         System.out.println(2*4*6*8*24*22);

