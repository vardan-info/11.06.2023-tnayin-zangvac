public class Main {
    public static void main(String[] args) {
        int[] array = new int[]{5,7,9,11,13,44};
        int count=0;
        for (int i = 0; i < array.length; i++) {
            if(array[i] %2==0){
                count++;
            }

        }
        System.out.println(count);


    }
}
