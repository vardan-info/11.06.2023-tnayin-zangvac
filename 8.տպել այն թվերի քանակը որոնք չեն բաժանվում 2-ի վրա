public class Main {
    public static void main(String[] args) {
        int[] array = new int[]{5,7,9,11,13,15};
        for (int i = 0; i < array.length; i++) {
            array[i] = array[i] / 2;
            System.out.println(array[i]);

        }
         System.out.println("i ---" + 5/2);
        System.out.println("i ---" +7/2);
        System.out.println("i ---" +9/2);
        System.out.println("i ---" +11/2);
        System.out.println("i ---" +13/2);
        System.out.println("i ---" +15/2);

    }
}
